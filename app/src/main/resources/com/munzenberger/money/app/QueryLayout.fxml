<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="container" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" spacing="6.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.munzenberger.money.app.QueryController">
   <children>
      <TextArea fx:id="queryTextArea" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER" />
      <HBox maxWidth="1.7976931348623157E308" spacing="12.0">
         <children>
            <Button fx:id="queryButton" mnemonicParsing="false" onAction="#onQueryButton" text="Execute Query" />
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#onUpdateButton" text="Execute Update" />
         </children>
      </HBox>
      <TableView fx:id="resultTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
