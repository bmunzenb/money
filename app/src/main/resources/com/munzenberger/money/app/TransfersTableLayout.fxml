<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.munzenberger.money.app.TransfersTableController">
   <children>
      <TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="categoryColumn" prefWidth="75.0" sortable="false" text="Category" />
          <TableColumn fx:id="memoColumn" prefWidth="75.0" sortable="false" text="Memo" />
            <TableColumn fx:id="amountColumn" prefWidth="75.0" sortable="false" style="-fx-alignment: CENTER-RIGHT;" text="Amount" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox fillHeight="false" maxWidth="1.7976931348623157E308" minWidth="0.0" spacing="6.0" VBox.vgrow="NEVER">
         <children>
            <ComboBox editable="true" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="0.0" prefWidth="200.0" promptText="Category" HBox.hgrow="SOMETIMES" />
            <TextField maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="0.0" prefWidth="200.0" promptText="Memo" HBox.hgrow="SOMETIMES" />
            <TextField maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="0.0" prefWidth="100.0" promptText="Amount" HBox.hgrow="SOMETIMES" />
            <Button mnemonicParsing="false" text="Add" HBox.hgrow="NEVER" />
         </children>
      </HBox>
   </children>
</VBox>
