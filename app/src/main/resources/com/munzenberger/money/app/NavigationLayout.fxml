<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.munzenberger.money.app.NavigationController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButton" text="&lt;" />
            <Button fx:id="forwardButton" mnemonicParsing="false" onAction="#onForwardButton" text="&gt;" />
            <Separator orientation="VERTICAL" />
            <Button mnemonicParsing="false" onAction="#onAccountsButton" text="Accounts" />
            <Button mnemonicParsing="false" onAction="#onCategoriesButton" text="Categories" />
            <Button mnemonicParsing="false" onAction="#onPayeesButton" text="Payees" />
            <Separator orientation="VERTICAL" />
            <Button mnemonicParsing="false" onAction="#onQueryButton" text="Query" />
        </items>
      </ToolBar>
   </top>
</BorderPane>
