<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.munzenberger.money.app.EditTransfersController">
   <children>
      <TableView fx:id="tableView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="categoryColumn" prefWidth="75.0" sortable="false" text="Category" />
          <TableColumn fx:id="memoColumn" prefWidth="75.0" sortable="false" text="Memo" />
            <TableColumn fx:id="amountColumn" prefWidth="75.0" sortable="false" style="-fx-alignment: CENTER-RIGHT;" text="Amount" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <VBox.margin>
            <Insets bottom="8.0" />
         </VBox.margin>
      </TableView>
      <HBox fillHeight="false" maxWidth="1.7976931348623157E308" minWidth="0.0" spacing="8.0" VBox.vgrow="NEVER">
         <children>
            <ComboBox fx:id="categoryComboBox" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="0.0" prefWidth="200.0" promptText="Category" HBox.hgrow="SOMETIMES" />
            <TextField fx:id="memoTextField" maxWidth="1.7976931348623157E308" minWidth="0.0" prefWidth="150.0" promptText="Memo" HBox.hgrow="SOMETIMES" />
            <TextField fx:id="amountTextField" maxWidth="1.7976931348623157E308" minWidth="0.0" prefWidth="100.0" promptText="Amount" HBox.hgrow="SOMETIMES" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButton" text="Add" HBox.hgrow="NEVER" />
         </children>
      </HBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" VBox.vgrow="NEVER">
        <buttons>
          <Button fx:id="doneButton" defaultButton="true" mnemonicParsing="false" onAction="#onDoneButton" text="Done" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancelButton" text="Cancel" />
        </buttons>
         <VBox.margin>
            <Insets top="12.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="18.0" left="18.0" right="18.0" top="18.0" />
   </padding>
</VBox>
